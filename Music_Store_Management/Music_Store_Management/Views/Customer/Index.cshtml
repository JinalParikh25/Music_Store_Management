@model Music_Store_Management.ViewModels.RandomViewModel

@{
    ViewData["Title"] = "Customer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Customers</h1>

<table class=“table table-bordered table-hover”>
    @{
        foreach(var customer in @Model.CustomerList)
        {
            <tr>
                <td>
                    @Html.ActionLink(@customer.Name, "Details", "Customer", new { id = customer.Id})
                </td>
            </tr>
        }
    }
   
</table>

